{
  "name": "expense-tracker-api",
  "version": "1.0.0",
  "description": "Serverless backend built using NestJs, DynamoDB, Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "jest",
    "webpack": "serverless webpack",
    "start": "serverless offline start",
    "start:api": "ts-node --type-check -r tsconfig-paths/register src/_DistributedServiceLayer/API/main.ts",
    "start:authapi": "ts-node --type-check -r tsconfig-paths/register src/_DistributedServiceLayer/API/auth.ts",
    "deploy": "serverless deploy --stage Dev  --verbose",
    "deploy:prod": "serverless deploy --stage Prod --verbose",
    "deploy:staging": "serverless deploy --aws-profile default --stage staging --verbose",
    "testapp": "ts-node testApp.ts",
    "dynamodb:create": "ts-node --type-check -r tsconfig-paths/register src/_DistributedServiceLayer/Cron-Job/Dynamodb/createTable.ts",
    "dynamodb:query": "ts-node --type-check -r tsconfig-paths/register src/_DistributedServiceLayer/Cron-Job/Dynamodb/query.ts",
    "dynamodb:load": "ts-node --type-check -r tsconfig-paths/register src/_DistributedServiceLayer/Cron-Job/Dynamodb/loadTable.ts",
    "remove": "serverless remove --stage Dev"
  },
  "dependencies": {
    "@nest-middlewares/helmet": "^5.1.1",
    "@nest-middlewares/method-override": "^5.1.1",
    "@nest-middlewares/morgan": "^5.1.1",
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "@nestjs/typeorm": "^6.2.0",
    "@sendgrid/mail": "^6.4.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node-fetch": "^2.5.2",
    "@types/request": "^2.48.3",
    "amazon-cognito-identity-js": "^3.1.3",
    "aws-sdk": "^2.554.0",
    "aws-serverless-express": "^3.3.6",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cross-fetch": "^3.0.4",
    "crypto-js": "^3.1.9-1",
    "crypto-random-string": "^3.0.1",
    "dottie": "^2.0.1",
    "express": "^4.17.1",
    "joi": "^14.0.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "node-fetch": "^2.6.0",
    "query-string": "^6.8.3",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.3",
    "winston": "^3.0.0",
    "winston-cloudwatch": "^2.0.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.4",
    "@types/aws-lambda": "^8.10.33",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/crypto-js": "^3.1.43",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/joi": "^14.0.0",
    "@types/node": "^10.14.22",
    "@types/request-promise": "^4.1.44",
    "@types/uuid": "^3.4.5",
    "copy-webpack-plugin": "^5.1.1",
    "jest": "^24.9.0",
    "serverless-dependson-plugin": "^1.1.2",
    "serverless-domain-manager": "^3.3.0",
    "serverless-nested-stack": "0.0.7",
    "serverless-offline": "^3.25.6",
    "serverless-plugin-split-stacks": "^1.9.1",
    "serverless-plugin-warmup": "^4.6.3-rc.1",
    "serverless-prune-plugin": "^1.4.2",
    "serverless-webpack": "^5.3.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
